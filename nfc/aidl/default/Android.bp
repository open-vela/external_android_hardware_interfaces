cc_binary {
    name: "android.hardware.nfc-service.example",
    relative_install_path: "hw",
    init_rc: ["nfc-service-example.rc"],
    vintf_fragments: ["nfc-service-example.xml"],
    vendor: true,
    cflags: [
        "-Wall",
        "-Wextra",
    ],
    shared_libs: [
        "libbase",
        "liblog",
        "libutils",
        "libbinder_ndk",
        "android.hardware.nfc-V1-ndk",
    ],
    srcs: [
        "main.cpp",
        "Nfc.cpp",
        "Vendor_hal_api.cpp",
    ],
}
